    "postcss-media-minmax": "^4.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-nesting": "^7.0.0",
    "postcss-selector-matches": "^4.0.0",
    "postcss-selector-not": "^4.0.0",
    "react-hot-loader": "^4.8.8",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "serve-static": "^1.14.1",
    "sitemap": "^2.2.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
